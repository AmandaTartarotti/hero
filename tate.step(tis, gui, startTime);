[32m+[m[32mimport com.googlecode.lanterna.input.KeyType;[m
[32m+[m[32mimport com.googlecode.lanterna.screen.Screen;[m
[32m+[m[32mimport com.googlecode.lanterna.screen.TerminalScreen;[m
[32m+[m[32mimport com.googlecode.lanterna.terminal.DefaultTerminalFactory;[m
[32m+[m[32mimport com.googlecode.lanterna.terminal.Terminal;[m
[32m+[m
[32m+[m[32mpublic class lanternaGui implements GUI {[m
[32m+[m
[32m+[m[32m    private Screen screen;[m
[32m+[m
[32m+[m[32m    public lanternaGui(Screen screen) {[m
[32m+[m[32m        this.screen = screen;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void LanternaGUI(int width, int height) throws IOException{[m
[32m+[m[32m        Terminal terminal = createTerminal(width, height);[m
[32m+[m[32m        this.screen = createScreen(terminal);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private Screen createScreen(Terminal terminal) throws IOException {[m
[32m+[m[32m        final Screen screen;[m
[32m+[m[32m        screen = new TerminalScreen(terminal);[m
[32m+[m
[32m+[m[32m        screen.setCursorPosition(null);[m
[32m+[m[32m        screen.startScreen();[m
[32m+[m[32m        screen.doResizeIfNecessary();[m
[32m+[m[32m        return screen;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private Terminal createTerminal(int width, int height) throws IOException {[m
[32m+[m[32m        TerminalSize terminalSize = new TerminalSize(width, height + 1);[m
[32m+[m[32m        DefaultTerminalFactory terminalFactory = new DefaultTerminalFactory()[m
[32m+[m[32m                .setInitialTerminalSize(terminalSize);[m
[32m+[m
[32m+[m[32m        Terminal terminal = terminalFactory.createTerminal();[m
[32m+[m[32m        return terminal;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public ACTION getNextAction() throws IOException {[m
[32m+[m[32m        KeyStroke keyStroke = screen.pollInput();[m
[32m+[m[32m        if (keyStroke == null) return ACTION.NONE;[m
[32m+[m
[32m+[m[32m        if (keyStroke.getKeyType() == KeyType.EOF) return ACTION.QUIT;[m
[32m+[m[32m        if (keyStroke.getKeyType() == KeyType.Character && keyStroke.getCharacter() == 'q') return ACTION.QUIT;[m
[32m+[m
[32m+[m[32m        if (keyStroke.getKeyType() == KeyType.ArrowUp) return ACTION.UP;[m
[32m+[m[32m        if (keyStroke.getKeyType() == KeyType.ArrowRight) return ACTION.RIGHT;[m
[32m+[m[32m        if (keyStroke.getKeyType() == KeyType.ArrowDown) return ACTION.DOWN;[m
[32m+[m[32m        if (keyStroke.getKeyType() == KeyType.ArrowLeft) return ACTION.LEFT;[m
[32m+[m
[32m+[m[32m        if (keyStroke.getKeyType() == KeyType.Enter) return ACTION.SELECT;[m
[32m+[m
[32m+[m[32m        return ACTION.NONE;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m    private void drawCharacter(int x, int y, char c, String color) {[m
[32m+[m[32m        TextGraphics tg = screen.newTextGraphics();[m
[32m+[m[32m        tg.setForegroundColor(TextColor.Factory.fromString(color));[m
[32m+[m[32m        tg.putString(x, y + 1, "" + c);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void drawHero(Position position) {[m
[32m+[m[32m        drawCharacter(position.getX(), position.getY(), 'H', "#FFD700");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void drawWall(Position position) {[m
[32m+[m[32m        drawCharacter(position.getX(), position.getY(), '#', "#3333FF");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void drawMonster(Position position) {[m
[32m+[m[32m        drawCharacter(position.getX(), position.getY(), '@', "#CC0000");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void drawCoin(Position position) {drawCharacter(position.getX(), position.getY(), '@', "#CC0000");}[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void clear() {[m
[32m+[m[32m        screen.clear();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void refresh() throws IOException {[m
[32m+[m[32m        screen.refresh();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void close() throws IOException {[m
[32m+[m[32m        screen.close();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
