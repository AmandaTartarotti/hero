[32m+[m[32mpackage State;[m
[32m+[m
[32m+[m[32mimport Gui.GUI;[m
[32m+[m[32mimport ModelController.Game;[m
[32m+[m[32mimport Viewer.Viewer;[m
[32m+[m
[32m+[m[32mimport java.io.IOException;[m
[32m+[m
[32m+[m[32mpublic abstract class State<T> {[m
[32m+[m[32m    private final T model;[m
[32m+[m[32m    private final Controller<T> controller;[m
[32m+[m[32m    private final Viewer<T> viewer;[m
[32m+[m
[32m+[m[32m    public State(T model) {[m
[32m+[m[32m        this.model = model;[m
[32m+[m[32m        this.viewer = getViewer();[m
[32m+[m[32m        this.controller = getController();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected abstract Viewer<T> getViewer();[m
[32m+[m
[32m+[m[32m    protected abstract Controller<T> getController();[m
[32m+[m
[32m+[m[32m    public T getModel() {[m
[32m+[m[32m        return model;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void step(Game game, GUI gui, long time) throws IOException {[m
[32m+[m[32m        GUI.ACTION action = gui.getNextAction();[m
[32m+[m[32m        controller.step(game, action, time);[m
[32m+[m[32m        viewer.draw(gui);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
